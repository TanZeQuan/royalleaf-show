{
  "expo": {
    "name": "",
    "slug": "royalleaf-premium",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/icons/login-logo.png",
    "scheme": "royalleafapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "src/assets/images/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/splashScreen.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.royalleaf.premium"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.royalleaf.premium",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "需要使用您的相机来拍照",
        "NSPhotoLibraryUsageDescription": "需要访问您的相册以选择照片",
        "NSPhotoLibraryAddUsageDescription": "需要保存图片到您的相册"
      }
    },
    "plugins": [
      "expo-localization",
      "expo-notifications"
    ],
    "extra": {
      "appDescription": "RoyalLeaf Premium 是专为奶茶爱好者设计的会员应用，提供会员积分、专属优惠及活动资讯。",
      "brandColor": "#4A5D23",
      "primaryFont": "Roboto-Regular"
    }
  }
}